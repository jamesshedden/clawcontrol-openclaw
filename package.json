{
  "name": "clawcontrol",
  "version": "1.0.0",
  "description": "OpenClaw channel plugin for ClawControl",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "openclaw": {
    "extensions": ["./index.ts"],
    "channel": {
      "id": "clawcontrol",
      "label": "ClawControl",
      "selectionLabel": "ClawControl (Desktop App)",
      "blurb": "Desktop notes app with AI chat.",
      "aliases": ["cc"]
    }
  },
  "license": "MIT"
}
